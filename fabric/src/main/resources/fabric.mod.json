{
  "schemaVersion": 1,
  "id": "rnd",
  "version": "${version}",
  "name": "Create: Research & Development",
  "description": "A gameplay overhaul for MC!",
  "authors": [
    "Potato",
    "R&D Team"
  ],
  "contributors": [
    "ewoudje",
    "triode",
    "rubydesic"
  ],
  "contact": {
    "homepage": "https://valkyrienskies.org/",
    "sources": "https://github.com/create-research-and-development-team/Research-And-Development"
  },
  "license": "Apache-2.0",
  "icon": "assets/rnd/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.valkyrienskies.rnd.fabric.VSCreateModFabric"
    ],
    "client": [
      "org.valkyrienskies.rnd.fabric.VSCreateModFabric${'$'}Client"
    ],
    "preLaunch": [
      "org.valkyrienskies.rnd.fabric.ValkyrienPreLaunch"
    ],
    "modmenu": [
      "org.valkyrienskies.rnd.fabric.VSCreateModFabric${'$'}ModMenu"
    ]
  },
  "mixins": [
    "rnd-common.mixins.json",
    "rnd.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.18.2",
    "valkyrienskies": ">=${vs2_version}"
  }
}
